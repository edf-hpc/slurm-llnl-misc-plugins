# Database name
ACCTDB=slurm_acct_db

# Base directory for SQL dumps
BKDIR=/var/backups/slurmdbd/database

# Database configuration file
DBMAINCONF=/etc/mysql/debian.cnf

# If 'false', the older dumps are deleted
KEEP_OLD=true

# if 'false', don't use single transaction
#  this locks the whole database during the
#  dump.
SINGLE_TRANSACTION=true
